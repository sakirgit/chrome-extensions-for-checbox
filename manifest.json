{
    "manifest_version": 3,
    "name": "H1 Title Collector",
    "description": "Collects the h1 title text from a list of specific URLs",
    "version": "1.0",
    "permissions": [
        "activeTab",
        "clipboardWrite"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    }
}

